<%- include('../inc/top',{title:'Chi tiết Chủ cửa hàng', adminLogin: adminLogin}) %>

    <div class="container mt-2">

        <div class="form-group row m-2">
            <label for="nameAdmin" class="col-sm-5 col-form-label" style="color: #848485;">Trang này để xem chi tiết
                Shop </label>
            <div class="col-sm-5">
            </div>
        </div>
        <form action="" method="get">

            <div class="form-group row m-3">
                <label for="username" form="bold" class="col-sm-2 col-form-label">Họ và tên</label>
                <div class="col-sm-3">
                    <label type="text" class="form-control" style="width: 200px;">
                        <%= objOwner.nameIdentity %>
                    </label>
                </div>

                <label for="username" form="bold" class="col-sm-2 col-form-label">Ngày sinh</label>
                <div class="col-sm-3">
                    <label type="text" style="width: 200px;" class="form-control">
                        <%= objOwner.dateIdentity %>
                    </label>
                </div>

            </div>

            <div class="form-group row m-3">
                <label for="username" form="bold" class="col-sm-2 col-form-label">Số căn cước</label>
                <div class="col-sm-3">
                    <label type="text" class="form-control" style="width: 200px;">
                        <%= objOwner.numberIdentity %>
                    </label>
                </div>
            </div>

            <div class="form-group row m-3" style="align-items: center;">
                <label for="username" form="bold" class="col-sm-2 col-form-label">Mặt trước căn cước</label>
                <div class="col-sm-3">
                    <% if (typeof(objOwner.imageIdentity[0]) !=='undefined' ) { %>

                        <img src="<%= objOwner.imageIdentity[0] %>" style="width: 250px; aspect-ratio: 3/2; border-radius: 10px;"
                            class="img-fluid">

                        <% } else { %>
                            <p>No images available.</p>
                            <% } %>
                </div>
                <label for="username" form="bold" class="col-sm-2 col-form-label">Mặt sau căn cước</label>
                <div class="col-sm-3">
                    <% if (typeof(objOwner.imageIdentity[1]) !=='undefined' ) { %>

                        <img src="<%= objOwner.imageIdentity[1] %>" style="width: 250px; aspect-ratio: 3/2; border-radius: 10px;"
                            class="img-fluid">

                        <% } else { %>
                            <p>No images available.</p>
                            <% } %>
                </div>
            </div>

    </div>
    <div class="form-group row m-3">
        <div class="col-sm-10" style="margin-left: 90px;">
            <input class="btn btn-secondary " type= 'button' style="background-color: #F582AE;"
            onclick='javascript:history.back();return false;' value='Quay lại'>
        </div>
    </div>
    </form>

    </div>

    <%- include('../inc/bottom') %>